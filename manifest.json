{
  "manifest_version": 2,

  "name": "Sorry Not Sorry Plugin",
  "description": "This extension highlights the word 'sorry' in your emails.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}